begin
  if CurStep = ssInstall then
    CreateDir(ExpandConstant('{app}\Install_Rus\'));
    RenameFile(ExpandConstant('{app}\Doki Doki Literature Club Plus_Data\Managed\SteamPlugin.dll'), ExpandConstant('{app}\Install_Rus\SteamPlugin.dll.bak'));
end;

