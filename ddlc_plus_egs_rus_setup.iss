function TryGetInstallDir(Param: String): String;
var
  S: String;  
  FileLines: TArrayOfString;
  LineCount: Integer;
  SectionLine: Integer;
  MyFileExists: Boolean;
begin
  MyFileExists := LoadStringsFromFile(Param, FileLines);
  LineCount := GetArrayLength(FileLines);
  if MyFileExists then
  begin
    for SectionLine := 0 to LineCount - 1 do
    begin
      if (Pos('DokiDokiLitClubPlus', FileLines[SectionLine]) <> 0) then
        S := Trim(FileLines[SectionLine]);
        StringChangeEx(S, '"InstallLocation": "', '', True);
        StringChangeEx(S, '",', '', True);
        StringChangeEx(S, '/', '\', True);
        Result := S;
    end;
  end
  else
  begin
    Result := 'C:\Program Files\Epic Games\DokiDokiLitClubPlus'
  end;
end;

